@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />

<div class="row" id="table-store">
    <div class="col-6 text-left ">
        <h2 class="text-primary">Historial de Inventario</h2>
    </div>
    <div class="col-3 text-right mt-3 ">
        <a class=" btn btn-primary" asp-action="NewInventory"> <i class="bi bi-plus"></i>Crear nuevo inventario</a>
    </div>
</div>

<br />


<div class="p-4 border rounded ">
    <table class="table" id="tbl-data" style=" width:100%">
        <thead>
            <tr>
                <th>Fecha de Inicio</th>
                <th>Fecha de Fin</th>
                <th>Tienda</th>
                <th>Usuario</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>


<div class="col-6 p-2">
    <a class="btn btn-success" asp-action="Index">
        Regresar
    </a>
</div>

@section Scripts {
    <script src="~/js/historic.js"></script>
}