@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<br />

<div class="row" id="table-store">
    <div class="col-6">
             <h2 class="text-primary"> Lista de Usuarios</h2>
    </div>
    
    <div class="col-6 text-right">
        <a class=" btn btn-primary" asp-area="Identity" asp-page="/Account/Register"> <i class="bi bi-plus"></i>Crear nuevo usuario</a>
    </div>
</div>

<br />

<div class="p-4 border rounded ">
    <table class="table" id="tbl-data" style=" width:100%">
        <thead>
            <tr>
                <th>Nombre</th>
                <th>Apellido</th>
                <th>Email</th>
                <th>Rol</th>
                <th class="text-center">Acceso</th>
            </tr>
        </thead>
    </table>
</div>






@*creo una seccion llmada Scripts que llamara a todos los scripst relacionados*@
@section Scripts{
    <script src="~/js/user.js"></script>
}